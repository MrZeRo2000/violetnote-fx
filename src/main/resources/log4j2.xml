<?xml version="1.0" encoding="UTF-8" ?>

<Configuration strict="true">
    <Appenders>
        <Appender type="Console" name="console">
            <Layout type="PatternLayout" pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p %c - %m%n"/>
        </Appender>
    </Appenders>
    <Loggers>
        <Logger name="com.romanpulov.violetnotefx">
        </Logger>
        <Root level="ERROR">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>
